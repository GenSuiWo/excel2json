{
  "name": "excel-to-json",
  "version": "1.0.0",
  "description": "Excel转JSON工具",
  "main": "app.js",
  "dependencies": {
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "axios": "^1.6.7",
    "xlsx": "^0.18.5"
  },
  "bin": {
    "excel-to-json": "./app.js"
  },
  "pkg": {
    "assets": [
      "node_modules/xlsx/**/*",
      "node_modules/ora/**/*",
      "node_modules/chalk/**/*"
    ],
    "targets": [
      "node16-win-x64",
      "node16-macos-x64",
      "node16-linux-x64"
    ]
  },
  "scripts": {
    "build": "pkg ."
  }
}
